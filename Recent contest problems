#include <iostream>
using namespace std;


int main() {
	// your code goes here
	 string str[1000];
	
	int T;
	cin>>T;
		

	for(int i=0;i<T;i++)
	{
	    int count1=0;
	    int count2=0;
	   
	       int N;
	        cin>>N;
	        for(int i=0;i<N;i++)
	        {
	            cin>>str[i];
	        }
	        for(int i=0;i<N;i++)
	        {
	            if(str[i]=="LTIME108")
	            {
	                count1++;
	               
	            }
	            else
	            {
	                count2++;
	            }
	        }
	    
	    cout<<count2<<" "<<count1<<endl;
	}
	return 0;
}
