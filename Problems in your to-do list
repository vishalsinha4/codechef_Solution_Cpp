#include <iostream>
using namespace std;

int main() {
	// your code goes here
	int n;
	cin>>n;
	int a[1000];
	
	for(int i=0;i<n;i++)
	{
	    int N;
	    cin>>N;
	    for(int i=0;i<N;i++)
	    {
	        cin>>a[i];
	    }
	    
	    int count=0;
	    for(int i=0;i<N;i++)
	    {
	        if(a[i]>=1000)
	        {
	            count++;
	        }
	        
	    }
	    cout<<count<<endl;
	    
	}
	return 0;
}
